{
    "basePath":"http://petstore.swagger.wordnik.com/api",
    "swaggerVersion":"1.2",
    "apiVersion":"1.0.0",
    "resourcePath":"/store",
    "apis":[
        {
            "path":"/store/order",
            "operations":[
                {
                    "method":"POST",
                    "summary":"Place an order for a pet",
                    "nickname":"placeOrder",
                    "type":"void",
                    "parameters":[
                        {
                            "paramType":"body",
                            "name":"body",
                            "type":"Order",
                            "required":true,
                            "allowMultiple":false,
                            "description":"order placed for purchasing the pet"
                        }
                    ],
                    "responseMessages":[
                        {
                            "code":400,
                            "message":"Invalid order"
                        }
                    ],
                    "notes":"",
                    "authorizations":{
                        "oauth2":[
                            {
                                "scope":"write:pets",
                                "description":"Modify pets in your account"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "path":"/store/order/{orderId}",
            "operations":[
                {
                    "method":"DELETE",
                    "summary":"Delete purchase order by ID",
                    "nickname":"deleteOrder",
                    "type":"void",
                    "parameters":[
                        {
                            "paramType":"path",
                            "name":"orderId",
                            "type":"string",
                            "required":true,
                            "allowMultiple":false,
                            "description":"ID of the order that needs to be deleted"
                        }
                    ],
                    "responseMessages":[
                        {
                            "code":400,
                            "message":"Invalid ID supplied"
                        },
                        {
                            "code":404,
                            "message":"Order not found"
                        }
                    ],
                    "notes":"For valid response try integer IDs with value < 1000.  Anything above 1000 or nonintegers will generate API errors",
                    "authorizations":{
                        "oauth2":[
                            {
                                "scope":"write:pets",
                                "description":"Modify pets in your account"
                            }
                        ]
                    }
                },
                {
                    "method":"GET",
                    "summary":"Find purchase order by ID",
                    "nickname":"getOrderById",
                    "type":"Order",
                    "parameters":[
                        {
                            "paramType":"path",
                            "name":"orderId",
                            "type":"string",
                            "required":true,
                            "allowMultiple":false,
                            "description":"ID of pet that needs to be fetched"
                        }
                    ],
                    "responseMessages":[
                        {
                            "code":400,
                            "message":"Invalid ID supplied"
                        },
                        {
                            "code":404,
                            "message":"Order not found"
                        }
                    ],
                    "notes":"For valid response try integer IDs with value <= 5. Anything above 5 or nonintegers will generate API errors",
                    "authorizations":{

                    }
                }
            ]
        }
    ],
    "models":{
        "Order":{
            "id":"Order",
            "properties":{
                "id":{
                    "type":"integer",
                    "format":"int64"
                },
                "petId":{
                    "type":"integer",
                    "format":"int64"
                },
                "quantity":{
                    "type":"integer",
                    "format":"int32"
                },
                "status":{
                    "description":"Order Status",
                    "type":"string",
                    "enum":[
                        "placed",
                        " approved",
                        " delivered"
                    ]
                },
                "shipDate":{
                    "type":"string",
                    "format":"date-time"
                }
            }
        }
    },
    "produces":[
        "application/json"
    ]
}