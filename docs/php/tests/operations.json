{
    "basePath":"http://example.com/api",
    "swaggerVersion":"1.2",
    "apiVersion":"1.0.0",
    "resourcePath":"/operations",
    "apis":[
        {
            "path":"/operations/pets",
            "operations":[
                {
                    "method":"GET",
                    "summary":"Retrieve all pets",
                    "nickname":"pets_0",
                    "type":"array[Pet]",
                    "parameters":[
                        {
                            "name":"name",
                            "type":"string"
                        },
                        {
                            "name":"status",
                            "type":"string"
                        }
                    ]
                },
                {
                    "method":"POST",
                    "summary":"Register a new pet",
                    "nickname":"pets_1",
                    "type":"Pet"
                }
            ]
        }
    ],
    "models":{
        "Pet":{
            "id":"Pet",
            "required":[
                "id",
                "name"
            ],
            "properties":{
                "id":{
                    "description":"unique identifier for the pet",
                    "type":"integer",
                    "format":"int64",
                    "minimum":"0.0",
                    "maximum":"100.0"
                },
                "name":{
                    "type":"string"
                },
                "category":{
                    "$ref":"Category"
                },
                "photoUrls":{
                    "type":"array",
                    "items":{
                        "type":"string"
                    }
                },
                "tags":{
                    "type":"array",
                    "items":{
                        "$ref":"Tag"
                    }
                },
                "status":{
                    "description":"pet status in the store",
                    "type":"string",
                    "enum":[
                        "available",
                        "pending",
                        "sold"
                    ]
                }
            }
        },
        "Category":{
            "id":"Category",
            "properties":{
                "id":{
                    "type":"integer",
                    "format":"int64"
                },
                "name":{
                    "type":"string"
                }
            }
        },
        "Tag":{
            "id":"Tag",
            "properties":{
                "id":{
                    "type":"integer",
                    "format":"int64"
                },
                "name":{
                    "type":"string"
                }
            }
        }
    }
}